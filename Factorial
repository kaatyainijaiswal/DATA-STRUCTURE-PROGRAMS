#include<stdio.h>
int fact (int);
int main()
   {
     int n,f;
     printf("enter the value \n");
     scanf("%d",&f);
     n=fact(f);
     printf("%d is the result\n",n);
     return 0;
  }
int fact(int f)
  { 
      if(f==0)
      return 1;
      else if(f==1)
      return 1;
      else 
      return f*fact(f-1);
  }
